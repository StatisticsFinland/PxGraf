{
  "LogOptions": {
    "SysId": "PxGraf",
    "AuthMethod": "Anonymous",
    "Level": "Error"
  },
  "ApplicationInsights": {
    "EnableAdaptiveSampling": false,
    "MinLevel": "Information"
  },
  "CacheOptions": {
    "Meta": {
      "SlidingExpirationMinutes": "1",
      "AbsoluteExpirationMinutes": "3"
    },
    "Data": {
      "SlidingExpirationMinutes": "30",
      "AbsoluteExpirationMinutes": "120"
    },
    "Database": {
      "AbsoluteExpirationMinutes": "5",
      "SlidingExpirationMinutes": "2",
      "ItemAmountLimit": "1000"
    },
    "Table": {
      "SlidingExpirationMinutes": "5"
    },
    "Visualization": {
      "SlidingExpirationMinutes": "60",
      "AbsoluteExpirationMinutes": "720",
      "ItemAmountLimit": "3000"
    },
    "CacheFreshnessCheckIntervalSeconds": "45"
  },
  "QueryOptions": {
    "MaxHeaderLength": 180,
    "MaxQuerySize": 100000
  },
  "Language": {
    "Default": "fi",
    "Available": [ "fi", "sv", "en" ]
  },
  "LocalFileSystemDatabaseConfig": {
    "Enabled": false,
    "DatabaseRootPath": "",
    "Encoding": "",
    "DatabaseWhitelist": []
  }
}