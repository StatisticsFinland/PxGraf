{
  "LogOptions": {
    "SysId": "PxGraf",
    "AuthMethod": "Anonymous",
    "Level": "Error"
  },
  "CacheOptions": {
    "Meta": {
      "SlidingExpiration": "600",
      "AbsoluteExpiration": "1800"
    },
    "Data": {
      "SlidingExpiration": "30",
      "AbsoluteExpiration": "120"
    },
    "Database": {
      "AbsoluteExpiration": "5"
    },
    "Table": {
      "SlidingExpiration": "5"
    },
    "Visualization": {
      "SlidingExpiration": "60",
      "AbsoluteExpiration": "720",
      "SizeLimit": "3000"
    },
    "CacheFreshnessCheckInterval": "45"
  },
  "QueryOptions": {
    "MaxHeaderLength": 180,
    "MaxQuerySize": 100000
  },
  "Language": {
    "Default": "fi",
    "Available": [ "fi", "sv", "en" ]
  }
}